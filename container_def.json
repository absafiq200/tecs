{
    "name":"${var.container_name}",
    "image":"${var.imageurl}",
    "essential": true,
    
    "portMappings": [{
        "ContainerPort": "${var.containerport}"
    }],
    
    "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
            "awslogs-group": "/ecs/td",
            "awslogs-region":"us-east-1",
            "awslogs-stream-prefix":"ecs"
        }
    }
}